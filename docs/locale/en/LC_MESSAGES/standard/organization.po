msgstr "|ocdsDescription| The street address. For example, 1600 Amphitheatre Pkwy."

msgid "locality"
msgstr ""

msgid "|ocdsDescription| Населений пункт. Наприклад, Київ."
msgstr "|ocdsDescription| The locality. For example, Mountain View."

msgid "region"
msgstr ""

msgid "|ocdsDescription| Область. Наприклад, Київська."
msgstr "|ocdsDescription| The region. For example, CA."

